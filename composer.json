{
	"name": "volponi/cakephp-cache",
	"description": "Cache plugin for CakePHP",
	"type": "cakephp-plugin",
	"license": "MIT",
	"require": {
		"php": ">=5.4.16",
		"cakephp/cakephp": "~3.0"
	},
	"require-dev": {
		"dereuromark/cakephp-tools": "dev-master",
		"fig-r/psr2r-sniffer": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Cache\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cache\\Test\\": "tests",
			"Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
		}
	}
}
